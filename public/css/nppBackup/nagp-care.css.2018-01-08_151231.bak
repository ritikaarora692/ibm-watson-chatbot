 .chatBox {
    width: 29%;
            display: flex;
            flex-direction: column;
            border: 1px solid #563d7c;
            height: 89vh;
            float: right;
                margin: -17px 4px 3px 8px;
    border-top-left-radius: 15px;
    border-top-right-radius: 15px;
    background-color: #fafafa;
}

.chatHeader {
    height: 30px;
    background-color: purple;
    border-top-left-radius: 15px;
    border-top-right-radius: 15px;
    text-align: center;
    color: white;
    vertical-align: middle;
    padding: 4px;
}

        .chatHistory {
            height: 100%;
            width: 100%;
            flex: 1;
            overflow-x: hidden;
            overflow-y: auto;
            font-size: 13px;
        }

        .chatInput {
            width: 100%;
            height: 40px;
        }

        #userInput,
        #sendButton {
            height: 40px;
            color: darkslategray;
            outline: 0;
        }

        .dialoginput {
            border: none;
            border-top: 2px solid #EAEAEA;
            border-bottom: 2px solid #EAEAEA;
            width: 90%;
            line-height: 50px;
            font-weight: bold;
            padding-left: 10px;
        }

        #sendButton {
            width: 10%;
            background: url(/assets/send-button.png) no-repeat center;
            background-size: 23px 28px;
            vertical-align: middle;
            float: right;    border: none;
            border-top: 2px solid #EAEAEA;
            border-bottom: 2px solid #EAEAEA;
            background-color: white;
        }

        .message_right,
        .sendee-user {
            text-align: right;
            width: 100%;
            color: azure;
        }

        .message_right:first-letter {
            text-transform: capitalize;
        }

        .message_left,
        .sendee-agent {
            text-align: left;
            width: 100%;
        }

        .sendee-agent,
        .sendee-user {
            font-weight: bold;
            margin-bottom: 5px;
        }

        .chatMessage {
            position: relative;
            overflow: hidden;
            border-radius: 10px;
            margin-bottom: 10px;
            padding: 5px;
        }

        .left {
            float: left;
        }

        .right {
            float: right;
        }

        .chatMessage.user {
            background-color: #9074bb;
            border: 1px solid #9074bb;
            border-radius: 50px;
            border-top-right-radius: 0;
            padding: 5px 14px 5px 5px;
            margin-right: 0.5em;
            margin-left: 30%;
        }

        .chatMessage.watson {
            background-color: #dad0e8;
            border: 1px solid #dad0e8;
            border-radius: 50px;
            border-top-left-radius: 0;
            padding: 5px 5px 13px 15px;
            margin-left: 0.5em;
            margin-right: 25%;
        }

        .navbar-inverse .navbar-nav>li>a {
            color: #cbbde2;
        }

        .navbar-inverse .navbar-nav>li>a.selected {
            color: white;
        }

        .navbar-inverse {
            background-color: #563d7c;
            border-color: #563d7c;
        }

        .d-inline-block {
            display: inline-block!important;
        }

        .align-top {
            vertical-align: top!important;
        }

        .margin-5 {
            margin-top: -5px;
        }

        .option a{
	            color: azure;
	        }

        .option {
            color: #fff;
    background-color: #563d7c;
    cursor: pointer;
    border-radius: 5.6px;
    box-shadow: 1px 1px 4px grey;
    border-width: 0px;
    border-top-right-radius: 1.3em;
    border-bottom-left-radius: 1.3em;
    border-top-left-radius: 1.3em;
    border-bottom-right-radius: 1.3em;
    padding: 3px 24px;
    margin: 2px;
    display: inline-block;
        font-size: 12px;
        }

        .option.doctor {
            font-size: 11px;
        }

        .option-list {
            list-style: none;
            padding: 5px;
        }

        #welcome {
            position: relative;
    display: block;
    padding: 10px 15px;
    line-height: 20px;
        padding-top: 16px;
    padding-bottom: 14px;
        font-size: 15px;
        text-transform: capitalize;
        }

        #map {
  height: 73%;
  width: 65%;
  float: left;
  margin-left: 25px;
}
html, body {
    height: 100%;
    margin: 0;
    padding: 0;
    background: #efefef;
}

.doc-section {
    float: left;
    width: 65%;
    padding-left: 25px;
}

.doc-section .doc-heading {
    text-align: justify;
    color: #563d7c;
    font-size: 23px;
    font-weight: bold;
}

.doc-section .doc-textarea {
    padding-top: 10px;
    padding-bottom: 10px;
}

@media (min-width: 768px) {
.navbar-nav {
    font-size: 17px;
}

.navbar-nav.logout-nav {
    float: right;
}
}

